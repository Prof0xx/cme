{
  "version": 2,
  "framework": "vite",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
        "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
      },
      "continue": true
    },
    {
      "src": "/api/service-requests",
      "dest": "/api/service-requests.ts"
    },
    {
      "src": "/api/services",
      "dest": "/api/services.ts"
    },
    {
      "src": "/api/leads",
      "dest": "/api/leads.ts"
    },
    {
      "src": "/api/referral",
      "dest": "/api/referral.ts"
    },
    {
      "src": "/api/admin",
      "dest": "/api/admin.ts"
    },
    {
      "src": "/api/auth",
      "dest": "/api/auth.ts"
    },
    {
      "src": "/api/referral-code/(.*)",
      "dest": "/api/referral.ts?path=codes&code=$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/sparkles.png",
      "dest": "/public/sparkles.png"
    },
    {
      "src": "/logo.png",
      "dest": "/public/logo.png"
    },
    {
      "src": "/logo-with-sparkles.png",
      "dest": "/public/logo-with-sparkles.png"
    },
    {
      "src": "/vercel.txt",
      "dest": "/public/vercel.txt"
    },
    {
      "src": "/branding/(.*)",
      "dest": "/public/branding/$1"
    },
    {
      "src": "/service-examples/(.*)",
      "dest": "/public/service-examples/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/(?!api/?.*$).*",
      "dest": "/index.html"
    }
  ]
}
